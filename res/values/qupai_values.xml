        <item name="android:activityCloseExitAnimation">@anim/activity_close_qupai_exit</item>
    </style>
    <style name="Qupai.Text.BalloonTip" parent="">
        <item name="android:textColor">@color/qupai_recording_tip_text_color</item>
    </style>
